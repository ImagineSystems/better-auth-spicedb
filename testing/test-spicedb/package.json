{
  "name": "test-spicedb",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "seed": "tsx scripts/seed.ts",
    "spicedb:start": "docker run --rm -p 50051:50051 authzed/spicedb serve --grpc-preshared-key testkey --grpc-no-tls",
    "spicedb:schema": "zed schema write --insecure --endpoint localhost:50051 --token testkey schema.zed",
    "spicedb:read": "zed schema read --insecure --endpoint localhost:50051 --token testkey",
    "spicedb:relationships": "zed relationship read --insecure --endpoint localhost:50051 --token testkey",
    "setup": "npm run db:push && npm run spicedb:schema && npm run seed",
    "test:alice": "zed permission check --insecure --endpoint localhost:50051 --token testkey agent:agent_1 view user:user_alice",
    "test:bob": "zed permission check --insecure --endpoint localhost:50051 --token testkey agent:agent_1 view user:user_bob"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/eslint": "^9.6.0",
    "drizzle-kit": "^0.28.1",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.36.0",
    "globals": "^15.0.0",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.3",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@authzed/authzed-node": "^1.1.0",
    "better-auth": "^1.0.0",
    "better-sqlite3": "^11.7.0",
	  "better-auth-spicedb": "link:../better-auth-spicedb",
    "drizzle-orm": "^0.36.4",
    "zod": "^3.23.8"
  },
  "type": "module"
}